{
  "name": "reactskelton",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "src/*.js",
      "src/*.jsx"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/jest/**/(*.)(spec|test).js?(x)"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageDirectory": "target/reports/coverage"
  },
  "scripts": {
    "build": "npm run compile",
    "clean:all": "rimraf ./package-lock.json && rimraf ./node_modules",
    "clean:install": "npm run clean:all && npm install",
    "clean:obsolete-jest-snapshots": "jest -u",
    "clean:obsolete-wdio-snapshots": "rimraf ./tests/wdio/**/__snapshots__ && npm run test:wdio",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "rimraf lib",
    "compile:build": "babel src --out-dir lib --copy-files",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint src/**/*.scss",
    "pack": "./node_modules/.bin/webpack-dev-server --config webpack.config.js -p",
    "prepush": "node scripts/prepush/index.js",
    "start": "webpack-dev-server --mode development",
    "test": "npm run lint && npm run test:jest && npm run test:wdio",
    "test:jest": "jest",
    "test:jest-coverage": "npm run test:jest -- --coverage",
    "test:lint": "npm run lint",
    "test:wdio": "wdio wdio.conf.js"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "axios-mock-adapter": "^1.15.0",
    "core-js": "^3.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.5.8",
    "react-router-dom": "^4.3.1",
    "redux-concatenate-reducers": "^1.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "wdio-chromedriver-service": "^0.1.3",
    "webdriverio": "^4.14.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "v8.0.0-beta.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.8.0",
    "react-on-rails": "7.0.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^1.4.5",
    "enzyme-redux": "^0.1.6",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.19.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.0.2",
    "postcss-custom-properties": "^6.0.1",
    "postcss-loader": "^2.0.5",
    "raf": "^3.4.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.8.0",
    "react-on-rails": "7.0.4",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "react-test-renderer": "^16.4.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
    "redux-test-utils": "^0.1.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "stylelint": "^8.0.0",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-sauce-service": "^0.4.14",
    "wdio-selenium-standalone-service": "~0.0.12"
  },
  "resolutions": {
    "**/babel-core": "^7.0.0-bridge.0"
  }
}
